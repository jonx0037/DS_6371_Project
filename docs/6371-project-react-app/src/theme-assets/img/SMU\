<script>
    // old js for the search form
    function SubmitSearch(sVal, site, e) {
        var keynum;
        keynum = 0;
    
        if(window.event) {
            keynum = e.keyCode;
        }
        else if(e.which) {
            keynum = e.which;
        }
        if (keynum == 13) {
            document.location.href='https://www.smu.edu/search?q=' + sVal;
            return false;
        }
    }

    $( document ).ready(function() {
        $('#searchbutton').on('click', function(event) {
            event.preventDefault();
            console.log('clicked');
            let sVal = $('#q2').val();
            document.location.href='https://www.smu.edu/search?q=' + sVal;
        });
    });

</script>

